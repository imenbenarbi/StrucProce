PROCEDURE dot_product(v1,v2:arry of INTEGER,var dot :INTEGER)
VAR
   i:INTEGER
BEGIN
    dot :=0
   IF (v1.length<>v2.length) THEN
    dot:=NaN
   ELSE
    FOR i FROM 0 TO v1.length STEP 1  DO
        dot:=dot+v1[i]*v2[i]
    END_FOR
   END_IF
END
